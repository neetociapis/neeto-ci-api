{
  "$schema": "https://mintlify.com/docs.json",
  "name": "NeetoCal API",
  "description": "NeetoCal API docs guide to manage scheduling links from external channels",
  "theme": "mint",
  "colors": {
    "primary": "#10B981",
    "light": "#34D399",
    "dark": "#059669"
  },
  "logo": "/assets/logo.svg",
  "favicon": "/assets/favicon.svg",
  "navbar": {
    "links": [
      {
        "label": "Blog",
        "href": "https://www.neeto.com/neetocal/blog"
      },
      {
        "label": "Help center",
        "href": "https://help.neetocal.com/"
      }
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/neetohq",
      "linkedin": "https://www.linkedin.com/company/neeto",
      "youtube": "https://www.youtube.com/channel/UCoQ3Kv2JdZ51WZJ3QFFcI9g",
      "slack": "https://www.launchpass.com/neetohq"
    }
  },
  "navigation": {
    "pages": [
      {
        "group": "Getting Started",
        "icon": "rocket",
        "pages": [
          "getting-started/introduction",
          "getting-started/identifying-subdomain",
          "getting-started/authentication",
          "getting-started/quickstart",
          "getting-started/pagination",
          "getting-started/response-codes"
        ]
      },
      {
        "group": "Scheduling links API",
        "icon": "calendar",
        "pages": [
          "api-reference/scheduling-links/create-meeting",
          "api-reference/scheduling-links/list-meetings",
          "api-reference/scheduling-links/get-meeting",
          "api-reference/scheduling-links/update-meeting",
          "api-reference/scheduling-links/create-one-off-link",
          "api-reference/scheduling-links/list-meeting-templates"
        ]
      },
      {
        "group": "Bookings API",
        "icon": "list-ol",
        "pages": [
          "api-reference/bookings/make-booking",
          "api-reference/bookings/list-bookings",
          "api-reference/bookings/get-booking",
          "api-reference/bookings/reschedule-booking",
          "api-reference/bookings/cancel-booking"
        ]
      },
      {
        "group": "Availabilities API",
        "icon": "clock",
        "pages": [
          "api-reference/availabilities/update-availability",
          "api-reference/availabilities/get-availability",
          "api-reference/availabilities/list-team-member-availabilities",
          "api-reference/availabilities/list-availabilities",
          "api-reference/availabilities/update-meeting-availability"
        ]
      },
      {
        "group": "Slots API",
        "icon": "user-clock",
        "pages": ["api-reference/slots/list-slots"]
      },
      {
        "group": "Packages API",
        "icon": "gift",
        "pages": [
          "api-reference/packages/list-packages",
          "api-reference/packages/get-package"
        ]
      },
      {
        "group": "Team Members API",
        "icon": "user-group",
        "pages": [
          "api-reference/team-members/list-team-members",
          "api-reference/team-members/add-team-members",
          "api-reference/team-members/update-team-member"
        ]
      },
      {
        "group": "Discount Codes API",
        "icon": "credit-card",
        "pages": ["api-reference/discount-codes/create-discount-code"]
      }
    ]
  },
  "api": {
    "params": {
      "expanded": "all"
    },
    "playground": {
      "display": "interactive",
      "proxy": true
    },
    "examples": {
      "languages": ["curl", "javascript", "python"],
      "defaults": "all"
    }
  },
  "styling": {
    "eyebrows": "section",
    "codeblocks": "system"
  },
  "appearance": {
    "default": "light",
    "strict": true
  }
}
